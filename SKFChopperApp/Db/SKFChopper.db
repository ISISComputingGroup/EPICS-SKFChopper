# ai record template for register inputs
# offset in 16bit words
record(ai, "$(P)SPEED") {
	field(DESC "Run Speed")
    field(DTYP,"asynFloat64")
    field(INP,"@asyn($(SPEED_L),0)FLOAT32_LE")
	field(SCAN, "I/O Intr")
	field(EGU, "Hz")
	field(PREC, "3")
	field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:SPEED")
    field(SDIS, "$(P)DISABLE")
}   

