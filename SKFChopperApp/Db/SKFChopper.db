
record(longin, "$(P)YEAR") {
    field(DESC, "Year in Chopper Memory")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(TIME_L),0)UINT16")
    field(SCAN, "I/O Intr")
    field(EGU, "")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:YEAR")
    field(SDIS, "$(P)DISABLE")
}   

record(longin, "$(P)MONTH") {
    field(DESC, "Month in Chopper Memory")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(TIME_L),1)UINT16")
    field(SCAN, "I/O Intr")
    field(EGU, "")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:MONTH")
    field(SDIS, "$(P)DISABLE")
}   

record(mbbi, "$(P)STATE") {
    field(DESC, "System State")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(STATE_L),0)UINT16")
    field(SCAN, "I/O Intr")
    field(SIML, "$(P)SIM")
    field(SIOL, 0)
	field(ZRVL, 0)
	field(ONVL, 1)
	field(TWVL, 2)
	field(THVL, 3)
	field(FRVL, 4)
	field(FVVL, 5)
	field(SXVL, 6)
	field(SVVL, 7)
	field(ZRST, "Invalid")
	field(ONST, "Power Up")
	field(TWST, "Standby")
	field(THST, "Idle")
	field(FRST, "Deleving")
	field(FVST, "Levitating")
	field(SXST, "Starting")
	field(SVST, "Stopping")
    field(SDIS, "$(P)DISABLE")
    info(INTEREST, "HIGH")
    info(archive, "VAL")
}

